{
  "name": "code-exercise",
  "version": "1.0.0",
  "description": "This is a shopping cart coding exercise",
  "main": "src/index.js",
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.13"
  },
  "scripts": {
    "old-heroku-prebuild": "npm i rimraf && npm run node-clean",
    "node-clean": "rimraf node_modules && mkdir node_modules && npm install && rimraf ./build && mkdir build",
    "heroku-prebuild": "rm -rf ./build",
    "heroku-postbuild": "npm run build-app",
    "build-app": "npm run client-build && npm run babel-build",
    "babel-build": "babel -d ./build ./src/server -s",
    "webpack": "webpack",
    "start": "node build/index.js",
    "client-dev": "webpack serve --mode development --env development --config webpack.dev.config.js",
    "server-dev": "nodemon",
    "node-start": "npm run run-local",
    "run-local": "babel-node src/server/index.js",
    "client-build": "webpack --mode production --env production --config webpack.prod.config.js",
    "dev-build": "webpack --mode development --env development --config webpack.dev.config.js",
    "test": "jest --config ./jest.config.js --coverage"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "bcrypt": "^5.0.1",
    "bootstrap": "^4.6.0",
    "canned": "^0.3.12",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^9.0.1",
    "cors": "^2.8.5",
    "css-loader": "^5.2.6",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "image-webpack-loader": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.0",
    "node-sass": "^6.0.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-autobind": "^1.0.6",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.9",
    "@testing-library/dom": "^8.1.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.24",
    "babel-jest": "^27.0.6",
    "dotenv": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "webpack-dev-server": "^3.11.2",
